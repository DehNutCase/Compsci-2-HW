#include "stdafx.h"
#include <vector>
#include <iostream>

std::vector<double> append(std::vector<double> a, std::vector<double> b) {
	std::vector<double> output;

	output = a;

	for (int i = 0; i < b.size(); i++) {
		output.push_back(b[i]);
	}

	return output;
}

int main()
{
	std::vector<double> c = { 1, 2, 3, 4, 5, 6, 7, 8 };

	std::vector<double> d = { 9, 10, 11, 12, 13, 14, 15, 16 };

	std::vector<double> e = append(c, d);


	for (int i = 0; i < e.size(); i++) {
		std::cout << e[i] << std::endl;
	}

	
	return 0;
}

